<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<Operation Class="PatchOperationSequence">
		<operations>
			<li Class="PatchOperationReplace">
				<xpath>/Defs/ResearchProjectDef[defName="VREA_AndroidTech"]/prerequisites</xpath>
				<value>
					<prerequisites>
						<li>AdvancedFabrication</li>
						<li>HighMechtech</li>
						<li>A2_AdvanceDroid</li>
					</prerequisites>
				</value>
			</li>
			<li Class="PatchOperationReplace">
				<xpath>/Defs/ResearchProjectDef[@Name="VREA_ResearchBase"]/tab</xpath>
				<value>
					<tab>Androids2</tab>
				</value>
			</li>
			<li Class="PatchOperationAdd">
				<xpath>Defs/VREAndroids.AndroidGeneDef[defName="VREA_PsychicallyDeaf"]</xpath>
				<value>
					<exclusionTags> <li>VREA_PsychicSensitivity</li> </exclusionTags>
				</value>
			</li>
			<li Class="PatchOperationRemove">
				<xpath>Defs/VREAndroids.AndroidSettings/androidsShouldNotReceiveHediffs/li[text()="PsychicAmplifier"]</xpath>
			</li>
		</operations>
	</Operation>
